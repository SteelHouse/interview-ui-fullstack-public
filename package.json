{
  "name": "mntnstarwars",
  "version": "0.0.1",
  "scripts": {
    "bootstrap": "npm ci && npm run prisma:setup && npm start",
    "start": "concurrently \"nodemon --exec ts-node ./server/server.ts\" \"npx cross-env PORT=5002 npm run client\"",
    "client": "npm start --prefix client",
    "interview": "npx ts-node ./scripts/interview.ts branch",
    "prisma:setup": "npx rimraf -rf ./prisma/migrations ./prisma/dev.db ./prisma/dev.db-journal && prisma migrate dev --name init",
    "generate:seedfile": "npx rimraf ./prisma/characters.json && ts-node prisma/character-json-seed.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^3.3.1",
    "pokedex-promise-v2": "^3.3.2",
    "prisma": "^4.13.0",
    "sqlite": "^4.2.0",
    "swapi-ts": "^1.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.1",
    "commander": "^10.0.1",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines" : { 
    "npm" : ">=9.6.0",
    "node" : ">=19.0.0"
  }
}
